#include<stdio.h>

int main(void)
{
  int i,j;
  int a[3][3],b[3][3],c[3][3];

   for(i=0;i<3;i++)
    {
      for(j=0;j<3;j++)
      {
        printf("enter the element in a[%d][%d]",i,j);
        scanf("%d",&a[i][j]);
      }
    }

    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
      {
        printf("enter the element in b[%d][%d]",i,j);
        scanf("%d",&b[i][j]);
      }
    }

    for(i=0;i<3;i++)
    {
     for(j=0;j<3;j++)
     {
       c[i][j] = a[i][j] + b[i][j];

     }
    }


   for(i=0;i<3;i++)
    {
     for(j=0;j<3;j++)
     {
          printf(" a[%d][%d] =%d + b[%d][%d]=%d =>c[%d][%d] =%d\n",i,j,a[i][j],i,j,b[i][j],i,j,c[i][j]);

     }
    }


 return 0;
}
